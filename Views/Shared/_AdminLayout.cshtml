﻿<!DOCTYPE html>
<html dir="ltr" lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="assets/images/favicon.png">
    <title>Matrix Template - The Ultimate Multipurpose admin template</title>
    <!--  Toaster Notifications-->
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" />

    <!-- Custom CSS -->
    <link href="~/assets/libs/fullcalendar/dist/fullcalendar.min.css" rel="stylesheet" />
    <link href="~/assets/extra-libs/calendar/calendar.css" rel="stylesheet" />
    <link href="~/dist/css/style.min.css" rel="stylesheet">
    <link href="~/Content/alertifyjs/alertify.min.css" rel="stylesheet" />
    <link href="~/Content/alertifyjs/themes/default.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>
<body>
    <div class="preloader">
        <div class="lds-ripple">
            <div class="lds-pos"></div>
            <div class="lds-pos"></div>
        </div>
    </div>
    <div id="main-wrapper">
        <header class="topbar" data-navbarbg="skin5">
            <nav class="navbar top-navbar navbar-expand-md navbar-dark">
                <div class="navbar-header" data-logobg="skin5">
                    <!-- This is for the sidebar toggle which is visible on mobile only -->
                    <a class="nav-toggler waves-effect waves-light d-block d-md-none" href="javascript:void(0)"><i class="ti-menu ti-close"></i></a>
                    <a asp-action="Index" asp-controller="Dashboard" class="navbar-brand">
                        <!-- Logo icon -->
                        <b class="logo-icon p-l-10">
                            <img src="~/assets/images/logo-icon.png" alt="homepage" class="light-logo" />
                        </b>
                        <span class="logo-text">
                            <!-- dark Logo text -->
                            <img src="~/assets/images/logo-text.png" alt="homepage" class="light-logo" />
                        </span>
                    </a>
                    <a class="topbartoggler d-block d-md-none waves-effect waves-light" href="javascript:void(0)" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><i class="ti-more"></i></a>
                </div>
                
            </nav>
        </header>
        <aside class="left-sidebar" data-sidebarbg="skin5">
            <!-- Sidebar scroll-->
            <div class="scroll-sidebar">
                <!-- Sidebar navigation-->
                <nav class="sidebar-nav">
                    <ul id="sidebarnav" class="p-t-30">

                        <li class="sidebar-item">
                            <a class="sidebar-link has-arrow waves-effect waves-dark" href="javascript:void(0)" aria-expanded="false"><i class="mdi mdi-school"></i><span class="hide-menu">Class </span></a>
                            <ul aria-expanded="false" class="collapse  first-level">
                                <li class="sidebar-item"><a href='@Url.Action("ViewClasses", "Class")' class="sidebar-link"><i class="fas fa-users"></i><span class="hide-menu">View All Classes</span></a></li>
                                <li class="sidebar-item"><a href='@Url.Action("ViewAllSubjects","Class")' class="sidebar-link"><i class="fas fa-book"></i><span class="hide-menu">View All Subjects </span></a></li>
                                <li class="sidebar-item"><a href='@Url.Action("CreateClass", "Class")' class="sidebar-link"><i class="fas fa-arrow-alt-circle-right"></i><span class="hide-menu"> Add Class </span></a></li>
                                <li class="sidebar-item"><a href='@Url.Action("AddSubjects", "Class")' class="sidebar-link"><i class="fas fa-arrow-alt-circle-right"></i><span class="hide-menu"> Add Subject </span></a></li>
                                <li class="sidebar-item"><a href='@Url.Action("ViewFees","Class")' class="sidebar-link"><i class="fas fa-list-alt"></i><span class="hide-menu"> Fee Setup </span></a></li>
                            </ul>
                        </li>


                        <li class="sidebar-item">
                            <a class="sidebar-link has-arrow waves-effect waves-dark" href="javascript:void(0)" aria-expanded="false"><i class="fas fa-user-graduate"></i><span class="hide-menu">Student </span></a>
                            <ul aria-expanded="false" class="collapse  first-level">
                                <li class="sidebar-item"><a href='@Url.Action("ViewAllStudents", "Student")' class="sidebar-link"><i class="fas fa-users"></i><span class="hide-menu">View All Students</span></a></li>
                                <li class="sidebar-item"><a href='@Url.Action("AddStudent", "Student")' class="sidebar-link"><i class="fas fa-arrow-alt-circle-right"></i><span class="hide-menu"> Add Student </span></a></li>
                            </ul>
                        </li>

                        <li class="sidebar-item">
                            <a class="sidebar-link has-arrow waves-effect waves-dark" href="javascript:void(0)" aria-expanded="false"><i class="fas fa-users-cog"></i><span class="hide-menu">Staff </span></a>
                            <ul aria-expanded="false" class="collapse  first-level">
                                <li class="sidebar-item"><a href='@Url.Action("ViewTeachers", "Teacher")' class="sidebar-link"><i class="fas fa-users"></i><span class="hide-menu">View All Staff</span></a></li>
                                <li class="sidebar-item"><a href='@Url.Action("AddTeacher", "Teacher")' class="sidebar-link"><i class="fas fa-arrow-alt-circle-right"></i><span class="hide-menu"> Add Staff </span></a></li>
                                <li class="sidebar-item"><a href='@Url.Action("AssignSubject", "Teacher")' class="sidebar-link"><i class="fas fa-arrow-alt-circle-right"></i><span class="hide-menu"> Assign Subject </span></a></li>
                                <li class="sidebar-item"><a href='@Url.Action("ViewAssignSubjects", "Teacher")' class="sidebar-link"><i class="fas fa-arrow-alt-circle-right"></i><span class="hide-menu">View Assigned Subjects </span></a></li>
                                <li class="sidebar-item"><a href='@Url.Action("ViewSalaries", "Teacher")' class="sidebar-link"><i class="fab fa-amazon-pay"></i><span class="hide-menu"> Staff Salary </span></a></li>
                                <li class="sidebar-item"><a href='@Url.Action("ViewAllStaffAttendance", "Teacher")' class="sidebar-link"><i class="fas fa-street-view"></i><span class="hide-menu"> Attendance </span></a></li>
                            </ul>
                        </li>
                        <li class="sidebar-item">
                            <a class="sidebar-link has-arrow waves-effect waves-dark" href="javascript:void(0)" aria-expanded="false"><i class="fas fa-users-cog"></i><span class="hide-menu">Parents </span></a>
                            <ul aria-expanded="false" class="collapse  first-level">
                                <li class="sidebar-item"><a href='@Url.Action("ViewAllClassesParents", "Parent")' class="sidebar-link"><i class="fas fa-users"></i><span class="hide-menu">View All Parents</span></a></li>
                                <li class="sidebar-item"><a href='@Url.Action("AddParent", "Parent")' class="sidebar-link"><i class="fas fa-arrow-alt-circle-right"></i><span class="hide-menu"> Add Parent </span></a></li>
                            </ul>
                        </li>
                        <li class="sidebar-item">
                            <a class="sidebar-link has-arrow waves-effect waves-dark" href="javascript:void(0)" aria-expanded="false"><i class="fas fa-list-alt"></i><span class="hide-menu">Finance</span></a>
                            <ul aria-expanded="false" class="collapse  first-level">
                                <li class="sidebar-item"><a href='@Url.Action("ViewExpenditures", "Expenditure")' class="sidebar-link"><i class="fas fa-arrow-alt-circle-right"></i><span class="hide-menu">View Expenditures</span></a></li>
                                <li class="sidebar-item"><a href='@Url.Action("CreateExpenditure", "Expenditure")' class="sidebar-link"><i class="fas fa-arrow-alt-circle-right"></i><span class="hide-menu"> Add Expenditures </span></a></li>
                                <li class="sidebar-item"><a href='@Url.Action("ViewIncome", "Expenditure")' class="sidebar-link"><i class="fas fa-arrow-alt-circle-right"></i><span class="hide-menu"> View Income </span></a></li>
                            </ul>
                        </li>
                        <li class="sidebar-item"> <a class="sidebar-link waves-effect waves-dark sidebar-link" href='@Url.Action("Login", "Authentication")' aria-expanded="false"><i class="fas fa-sign-out-alt"></i><span class="hide-menu">Logout</span></a></li>
                    </ul>
                </nav>
            </div>
        </aside>
        <partial name="_NotificationLayout" />
        @RenderBody()
    </div>
    <script src="~/assets/libs/jquery/dist/jquery.min.js"></script>
    <script src="~/dist/js/jquery.ui.touch-punch-improved.js"></script>
    <script src="~/dist/js/jquery-ui.min.js"></script>
    <!-- Bootstrap tether Core JavaScript -->
    <script src="~/assets/libs/popper.js/dist/umd/popper.min.js"></script>
    <script src="~/assets/libs/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- slimscrollbar scrollbar JavaScript -->
    <script src="~/assets/libs/perfect-scrollbar/dist/perfect-scrollbar.jquery.min.js"></script>
    <script src="~/assets/extra-libs/sparkline/sparkline.js"></script>
    <!--Wave Effects -->
    <script src="~/dist/js/waves.js"></script>
    <!--Menu sidebar -->
    <script src="~/dist/js/sidebarmenu.js"></script>
    <!--Custom JavaScript -->
    <script src="~/dist/js/custom.min.js"></script>
    <!-- this page js -->
    <script src="~/assets/libs/moment/min/moment.min.js"></script>
    <script src="~/assets/libs/fullcalendar/dist/fullcalendar.min.js"></script>
    <script src="~/dist/js/pages/calendar/cal-init.js"></script>
    <script src="~/Scripts/alertify.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>